{
  "name": "@superbrave/dok-search-vue",
  "version": "1.1.5",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/superbrave/frontend-search-component"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "module": "vue-cli-service build --target lib --name sbdoksearch ./src/main.esm.js",
    "web": "vue-cli-service build --target wc --name sb-search-wc ./src/main.wc.js",
    "publish:npm": "yarn module && npm publish",
    "release": "yarn module && release-it"
  },
  "main": "./dist/sbdoksearch.common.js",
  "files": [
    "dist/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "@elastic/search-ui": "^1.3.3",
    "@elastic/search-ui-app-search-connector": "^1.3.3",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^3.3.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-e2e-cypress": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^4.14.1",
    "prettier": "^2.0.5",
    "release-it": "^14.11.6",
    "sass-loader": "^8.0.2",
    "superbrave-utility-framework": "^0.0.20",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "prettier": {},
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not dead"
  ],
  "license": "GPL-3.0"
}
